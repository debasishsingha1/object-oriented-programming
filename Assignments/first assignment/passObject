package Assignments;

//passing objects as constructor
class Add1 {
    int a;
    int b;
    int c;
    Add1 (int i,int j,int k){
    a = i;
    b = j;
    c = k;
    }
    int result(){
    return a+b+c;
    }
    }
    class Addition{
        public static void main(String[] args) {
           Add1 firstexample = new Add1(5,10,15);
           int res;
           res = firstexample.result();
           System.out.println("Addition result: " + res);
        }
}
